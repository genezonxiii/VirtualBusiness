	<queryString>
		<![CDATA[select detail.group_id, detail.c_product_id, detail.product_name, detail.quantity,
pf.platform_name, ext.deliver_name, ext.deliver_note, rs.order_no
from tb_shipDetail detail
left join tb_realsaleDetail rsDetail on detail.realsaleDetail_id = rsDetail.realsaleDetail_id
left join tb_realsale rs on rs.realsale_id = rsDetail.realsale_id
left join tb_sale_ext ext on ext.sale_id = rs.realsale_id
left join tb_platform pf on pf.platform_code = rs.order_source
where detail.group_id = $P{group_id}
and rsDetail.order_no = $P{order_no}]]>
	</queryString>
